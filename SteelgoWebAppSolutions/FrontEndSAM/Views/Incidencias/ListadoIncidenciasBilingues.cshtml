    #grid > .k-grid-header > div > table,
    #grid > .k-grid-content > table {
        width: 100% !important;
    }
</style>*@

@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="ListadoIncidencias0001"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("ListadoIncidenciasBiligues", "Incidencias")"><span id="IncidenciasBilinguesBreadcrum"></span></a>
    </li>
}
<div class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar" class="accionGuardar"><span id="botonGuardar2"></span></a></li>
                            @*<li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>*@
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="EntregaresultadosDivSup" class="button-section">
                    <a id="EntregaresultadosSup" class="btn btn-primary"><span id="ServiciosTecnicosCapturaReporte"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div id="FieldSetView"  class="row ">
        <div class="row addedSectionInLineWithoutColor">
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
                <label id="ListadoIncidencias0005"></label>
                <input id="ProyectoID" class="" />
            </div>
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
                <label id="">Etapa</label>
                <input id="EtapaID" class="" />
            </div>
            
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3 altoControl" id="MuestraDiv">
                <label id="lblMostrar"></label>
                <input type="radio" name="Muestra" class="radio-responsive" style="width:auto;" value="0" /><span id="lblSinCaptura"></span>
                <input type="radio" name="Muestra" class="radio-responsive" style="width:auto;" value="1" checked/><span id="lblTodos"> </span>
            </div>
            <div class="col-xs-12 col-sm-2 col-md-6 col-lg-3">
                <label id="">&nbsp;</label>
                <button class="btn btn-blue" id="btnBuscar">
                    <span id="botonMostrar"></span>
                </button>
            </div>
        </div>
        <div class="row addedSectionInLineWithoutColor">
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
                <label id="ListadoIncidencias0014"></label>
                <input id="PeriodoTiempo" class="" />
            </div>

            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
                <label id="ListadoIncidencias0006"></label>
                <input id="FechaInicial" class="" />
            </div>
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
                <label id="ListadoIncidencias0007"></label>
                <input id="FechaFinal" class="" />
            </div>
        </div>

    </div>
    @*GRID*@
    <div class="row addedSectionInLine">
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2 altoControl" id="planchadoZone">
            <label id="lblPlanchar"></label>
            <input type="radio" name="LLena" class="radio-responsive" style=" width:auto;" value="Todos" /><span id="lblTodos1"></span>
            <input type="radio" name="LLena" class="radio-responsive" style=" width:auto;" value="Vacios" checked/><span id="lblVacios"></span>
        </div>
        <div id="divTraducionTItulo" class="form-group col-xs-12 col-sm-6 col-md-1 col-lg-1">
            <label id="">Traducir</label>
            <button id="TraduceES" class="btn-danger btn-sm"><img src="~/Content/images/flags/en-US.png" /></button>
        </div>
        <div id="ContenedorGrid" class="col-xs-12 col-sm-12 col-md-12 col-lg-12" >
            <div id="grid" data-role="grid" class="k-grid k-widget">
            </div>
        </div>
        
    </div>

    <div class="row ">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="GuardarPie" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar1" class="accionGuardar"><span id="botonGuardar4"></span></a></li>
                            @*<li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>*@
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>
                </div>
                <div id="EntregaresultadosDivINF" class="button-section">
                    <a id="EntregaresultadosINF" class="btn btn-primary"><span id="ServiciosTecnicosCapturaReporte1"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>


            </div>
        </div>
    </div>
</div>

